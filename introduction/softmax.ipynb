   "source": [
    "## defination of softmax\n",
    "##softmax(zi)=exp(zi)/âˆ‘jexp(zj)\n",
    "\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def softmax(arr):\n",
    "    expr = np.exp(arr)\n",
    "    expr_sum = np.sum(np.exp(arr))\n",
    "    return expr/expr_sum"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "tags": []
   },
   "outputs": [
    {
